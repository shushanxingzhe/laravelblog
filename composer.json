{
	"name": "shushanxingzhe/laravelblog",
	"version": "1.0.0",
	"description": "shushanxingzhe blog user the Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "4.2.*",
		"cartalyst/sentry": "2.1.4",
		"edvinaskrucas/notification": "3.0.1"
	},
	"require-dev": {
		"way/generators": "~2.0",
		"fzaninotto/faker": "1.3.0"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/services"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"extra": {
	        "branch-alias": {
	            "dev-master": "1.0.0-dev"
	        }
    	},
	"minimum-stability": "stable"
}
